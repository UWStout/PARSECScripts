[loggers]
keys=root,MetaPy

[handlers]
keys=stdoutHandler

[formatters]
keys=simpleFormatter

[logger_root]
level=DEBUG
handlers=stdoutHandler

[logger_MetaPy]
level=DEBUG
qualname=MetaPy
handlers=stdoutHandler
propagate=0

[handler_stdoutHandler]
class=StreamHandler
level=DEBUG
formatter=simpleFormatter
args=(sys.stdout,)

[formatter_simpleFormatter]
format=%(asctime)s - %(name)s - %(levelname)s - %(message)s
datefmt=
